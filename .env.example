# daneel-web environment variables
# Copy to .env and adjust as needed. All values shown are defaults.

# Redis connection (read-only access to DANEEL cognitive streams)
REDIS_URL=redis://localhost:6379

# Qdrant vector database (read-only access to memory collections)
QDRANT_URL=http://localhost:6334

# DANEEL core API (extended metrics, injection proxy, auth validation)
DANEEL_CORE_URL=http://localhost:3030

# HTTP listen port
PORT=3000

# Path to the compiled Leptos/WASM frontend assets
FRONTEND_DIR=./frontend/dist

# Logging level (tracing-subscriber env filter)
# Default when unset: daneel_web=info,tower_http=debug
RUST_LOG=daneel_web=info,tower_http=debug
